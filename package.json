    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "clean": "rm -rf .next node_modules",
    "changelog": "git-cliff --output CHANGELOG.md",
    "changelog:preview": "git-cliff --unreleased",
    "version:patch": "npm version patch --no-git-tag-version && npm run changelog",
    "version:minor": "npm version minor --no-git-tag-version && npm run changelog",
    "version:major": "npm version major --no-git-tag-version && npm run changelog",
    "release": "npm run changelog && git add CHANGELOG.md package.json && git commit -m \"chore(release): v$(node -p \"require('./package.json').version\")\" && git tag v$(node -p \"require('./package.json').version\")"
